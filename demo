public class VariablesExample {
    public static void main(String[] args) {
        int age = 20;
        double height = 5.9;
        String name = "Sugar";
        boolean isStudent = true;

        System.out.println(name + " is " + age + " years old and " + (isStudent ? "still grinding through school." : "done with school!"));
    }
}
